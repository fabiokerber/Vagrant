Vagrant.configure("2") do |config|
    config.vm.box = "ubuntu/bionic64"
    config.vm.network "forwarded_port", guest: 80, host:8089 #PORT FORWARD 8089 FÃSICA PARA 80 VIRTUAL
    config.vm.network "public_network", ip: "192.168.0.50"
    
    config.vm.provision "shell", inline: "cat /configs/id_bionic.pub >> .ssh/authorized_keys"
    
    config.vm.synced_folder "./configs", "/configs"
    config.vm.synced_folder ".", "/vagrant", disabled: true
end